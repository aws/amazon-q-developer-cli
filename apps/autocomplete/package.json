  "version": "1.187.0",
  "autocomplete-engine-version": "v9",
  "private": true,
  "type": "module",
  "scripts": {
    "set-host": "fig settings developer.autocomplete.host http://localhost:3124",
    "remove-host": "fig settings developer.autocomplete.host --delete",
    "dev": "vite",
