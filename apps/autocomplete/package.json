{
  "name": "autocomplete-engine-react",
  "version": "1.187.0",
  "autocomplete-engine-version": "v9",
  "private": true,
  "type": "module",
  "scripts": {
    "set-host": "fig settings developer.autocomplete.host http://localhost:3124",
    "remove-host": "fig settings developer.autocomplete.host --delete",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "serve": "vite preview --port 3124",
    "test": "vitest",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives && prettier --check src",
    "lint:fix": "eslint src --fix && prettier --write src",
    "clean": "rm -rf dist"
  },
  "browserslist": [
    "defaults",
    "Safari >= 11"
  ],
  "dependencies": {
    "@amzn/fig-io-api-bindings-wrappers": "workspace:^",
    "@amzn/fig-io-api-client": "workspace:^",
    "@amzn/fig-io-autocomplete-parser": "workspace:^",
    "@amzn/fig-io-shell-parser": "workspace:^",
    "@fig/autocomplete-helpers": "^1.0.7",
    "@fig/autocomplete-shared": "^1.1.2",
    "@fig/fig-api-proto": "workspace:^",
    "@juggle/resize-observer": "^3.4.0",
    "@types/js-yaml": "^4.0.9",
    "@vitejs/plugin-react": "^4.2.1",
    "@withfig/api-bindings": "workspace:~",
    "@withfig/autocomplete-types": "^1.31.0",
    "deep-object-diff": "^1.1.9",
    "exponential-backoff": "^3.1.1",
    "js-yaml": "^4.1.0",
    "loglevel": "^1.8.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-shadow": "^20.4.0",
    "react-window": "^1.8.10",
    "semver": "^7.5.4",
    "tailwind-merge": "^2.1.0",
    "use-resize-observer": "^9.1.0",
    "util": "^0.12.5",
    "vite": "^5.2.8",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@amzn/fig-io-eslint-config": "workspace:^",
    "@internal/fuzzysort": "workspace:^",
    "@internal/shared": "workspace:^",
    "@internal/types": "workspace:^",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "@types/react-window": "^1.8.8",
    "@types/semver": "^7.5.6",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@vitejs/plugin-legacy": "^5.2.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "postcss": "^8.4.32",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.3.6",
    "terser": "^5.26.0",
    "typescript": "^5.3.3",
    "vitest": "^1.6.0"
  },
  "bundledDependencies": [
    "@internal/fuzzysort",
    "@internal/shared"
  ]
}
