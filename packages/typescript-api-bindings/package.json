{
  "name": "@withfig/api-bindings",
  "version": "0.30.3",
  "license": "MIT",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "typings": "./dist/cjs/index.d.ts",
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/esm/index.js"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "clean": "rm -rf dist/",
    "build": "npm run clean && npm run generate-requests && npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc --declaration --outDir dist/cjs",
    "build:esm": "tsc --declaration --module esnext --outDir dist/esm",
    "lint": "eslint src && prettier --check src",
    "lint:fix": "eslint --fix src && prettier --write src",
    "precommit": "lint-staged",
    "prepublishOnly": "npm run build",
    "generate-requests": "tsx codegen/generate-requests.ts && prettier -w src/requests.ts"
  },
  "author": "The Fig Team",
  "dependencies": {
    "@fig/fig-api-proto": "workspace:^"
  },
  "devDependencies": {
    "@amzn/fig-io-eslint-config": "workspace:^",
    "@tsconfig/recommended": "^1.0.6",
    "@types/node": "^20.12.11",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "@typescript/analyze-trace": "^0.10.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "ts-morph": "^22.0.0",
    "ts-proto": "~1.174.0",
    "tslib": "^2.6.2",
    "tsx": "^4.10.2",
    "typescript": "^5.4.5"
  },
  "lint-staged": {
    "*.{ts,tsx}": "npx eslint --cache --fix"
  }
}
