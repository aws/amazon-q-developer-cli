{
  "name": "@withfig/api-bindings",
  "version": "0.30.3",
  "license": "MIT",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "typings": "./dist/cjs/index.d.ts",
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/esm/index.js"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "clean": "rm -rf dist/",
    "build": "npm run clean && npm run generate-requests && npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc --declaration --outDir dist/cjs",
    "build:esm": "tsc --declaration --module esnext --outDir dist/esm",
    "lint": "eslint src",
    "lint:fix": "npm run lint --fix",
    "precommit": "lint-staged",
    "prepublishOnly": "npm run build",
    "generate-requests": "npx ts-node -O '{\"module\":\"commonjs\"}' codegen/generate-requests.ts"
  },
  "author": "The Fig Team",
  "dependencies": {
    "@fig/fig-api-proto": "workspace:^"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.2",
    "@types/node": "^20.6.0",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "@typescript/analyze-trace": "^0.10.1",
    "eslint": "^8.49.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-import": "^2.28.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.0",
    "ts-morph": "^19.0.0",
    "ts-node": "^10.9.1",
    "ts-proto": "~1.159.1",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6"
  },
  "lint-staged": {
    "*.{ts,tsx}": "npx eslint --cache --fix"
  }
}
