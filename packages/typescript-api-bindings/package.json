{
  "name": "@withfig/api-bindings",
  "version": "0.30.3",
  "license": "MIT",
  "type": "module",
  "exports": "./dist/index.js",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "clean": "rm -rf dist/",
    "build": "npm run clean && npm run generate-requests && tsc",
    "lint": "eslint src && prettier --check src",
    "lint:fix": "eslint --fix src && prettier --write src",
    "precommit": "lint-staged",
    "prepublishOnly": "npm run build",
    "generate-requests": "tsx codegen/generate-requests.ts && prettier -w src/requests.ts"
  },
  "author": "The Fig Team",
  "dependencies": {
    "@fig/fig-api-proto": "workspace:^"
  },
  "devDependencies": {
    "@amzn/fig-io-eslint-config": "workspace:^",
    "@internal/tsconfig": "workspace:^",
    "@tsconfig/recommended": "^1.0.6",
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "@typescript/analyze-trace": "^0.10.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "lint-staged": "^15.2.5",
    "prettier": "^3.2.5",
    "ts-morph": "^22.0.0",
    "ts-proto": "~1.176.1",
    "tslib": "^2.6.2",
    "tsx": "^4.11.0",
    "typescript": "^5.4.5"
  },
  "lint-staged": {
    "*.{ts,tsx}": "npx eslint --cache --fix"
  }
}
